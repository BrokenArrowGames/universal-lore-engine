{
  "name": "universal-lore-engine",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "start:debug": "nest start --debug --watch",
    "test": "jest --config ./test/jest.config.ts",
    "test:cov": "pnpm test -- --coverage && ts-node scripts/generate-badge.ts",
    "test:e2e": "jest --config ./test/jest-e2e.config.ts",
    "test:int": "jest --config ./test/jest-int.config.ts",
    "orm:show": "pnpm typeorm-ts-node-commonjs -d config/db-${npm_config_type}.ts migration:show",
    "orm:run": "pnpm typeorm-ts-node-commonjs -d config/db-${npm_config_type}.ts migration:run",
    "orm:revert": "pnpm typeorm-ts-node-commonjs -d config/db-${npm_config_type}.ts migration:revert",
    "orm:ddl": "pnpm typeorm-ts-node-commonjs -d config/db-migration.ts migration:generate src/database/migration/${npm_package_version}/${npm_config_name}",
    "orm:dml": "pnpm typeorm-ts-node-commonjs migration:create src/database/seed/${npm_package_version}/${npm_config_name}"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.616.0",
    "@casl/ability": "^6.7.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "class-validator": "^0.14.1",
    "express-session": "^1.18.0",
    "pg": "^8.12.0",
    "pino": "^9.3.1",
    "pino-abstract-transport": "^1.2.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@automock/adapters.nestjs": "^2.1.0",
    "@automock/jest": "^2.1.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.14.12",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "cognito-local": "^3.23.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "pino-pretty": "^11.2.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  },
  "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903",
  "pnpm": {
    "overrides": {
      "fast-xml-parser@<4.4.1": ">=4.4.1"
    }
  }
}
